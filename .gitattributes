# Auto detect text files and perform LF normalization
* text=auto

# Source code
*.rs text eol=lf
*.toml text eol=lf
*.md text eol=lf
*.json text eol=lf
*.yaml text eol=lf
*.yml text eol=lf

# Scripts
*.sh text eol=lf
*.py text eol=lf

# Documentation
*.txt text eol=lf

# Windows files
*.bat text eol=crlf
*.cmd text eol=crlf

# Binary files
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.ico binary
*.mov binary
*.mp4 binary
*.mp3 binary
*.flv binary
*.fla binary
*.swf binary
*.gz binary
*.zip binary
*.7z binary
*.ttf binary
*.eot binary
*.woff binary
*.woff2 binary
*.pyc binary
*.pdf binary
*.onnx binary
*.pb binary

# Export-ignore for archives
.gitattributes export-ignore
.gitignore export-ignore
.github export-ignore
